@echo off
REM  Script for releasing the device from isolation state

setlocal EnableDelayedExpansion

echo Starting Unisolation... 

REM  Add registry blob
REG add "HKLM\SOFTWARE\Policies\Microsoft\Windows Advanced Threat Protection" /v offlineCommand /t REG_SZ /f /d "{\"body\":\"{\\\"cmd\\\":{\\\"type\\\":\\\"unisolationcommand\\\",\\\"cmdtype\\\":11,\\\"id\\\":null,\\\"priority\\\":0,\\\"timestamp\\\":638755859276173217,\\\"enforce\\\":true},\\\"machineid\\\":\\\"a526b25d7eaa6ac37718e04493ae8f30e7e7d724\\\",\\\"tenantid\\\":\\\"04206fe5-5b4f-46b1-abdf-cb2fb558c6a8\\\",\\\"creationtimestamp\\\":638755859276181916,\\\"expirationTimestamp\\\":133847219249360000}\",\"sig\":\"\",\"sha256sig\":\"\",\"cert\":\"\",\"chain\":[\"\",\"\"]}" >NUL 2>&1
if !ERRORLEVEL! NEQ 0 (
   echo Unable to apply unisolate command, access denied
   goto ERROR
)

timeout 7 >NUL 2>&1

if [%PROCESSOR_ARCHITEW6432%] EQU [] (
  set powershellPath=%windir%\System32\WindowsPowerShell\v1.0\powershell.exe
) else (
  set powershellPath=%windir%\SysNative\WindowsPowerShell\v1.0\powershell.exe
)


REM  Verify unisolate event was raised
for /l %%x in (1, 1, 3) do (
    %powershellPath% -ExecutionPolicy Bypass -NoProfile -Command "$successList = Get-WinEvent -LogName Microsoft-Windows-SENSE/Operational | Where-Object {$_.Id -eq 71 -and $_.Message -match \"unisolation\" -and $_.TimeCreated -gt (Get-Date).AddMinutes(-5)};if($successList){Exit 1} else {Exit 0}"
    if !ERRORLEVEL! NEQ 0 (
       goto SUCCESS
    )
    
    %powershellPath% -ExecutionPolicy Bypass -NoProfile -Command "$failedList = Get-WinEvent -LogName Microsoft-Windows-SENSE/Operational | Where-Object {$_.Id -eq 60 -and $_.Message -match \"unisolation\" -and $_.TimeCreated -gt (Get-Date).AddMinutes(-5)};if($failedList){Exit 1} else {Exit 0}" 
    if !ERRORLEVEL! NEQ 0 (
       echo Unisolation failed with exit code 1
       goto ERROR
    )    
    
    timeout 7 >NUL 2>&1
)

echo Unisolation failed with exit code 2

goto ERROR

:SUCCESS
  echo Unisolation completed.

:ERROR
